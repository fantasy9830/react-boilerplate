{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React Boilerplate",
  "author": "Ricky <fantasy9830@gmail.com>",
  "private": true,
  "dependencies": {
    "ant-design-pro": "2.2.1",
    "antd": "3.26.6",
    "axios": "0.19.1",
    "dayjs": "1.8.19",
    "i18next": "19.0.3",
    "i18next-browser-languagedetector": "4.0.1",
    "js-cookie": "^2.2.1",
    "jwt-decode": "2.2.0",
    "nprogress": "0.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-i18next": "11.2.7",
    "react-icons": "3.8.0",
    "react-media": "1.10.0",
    "react-redux": "7.1.3",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.3.0",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "sanitize.css": "11.0.0",
    "source-map-explorer": "^2.2.1",
    "styled-components": "4.4.1"
  },
  "devDependencies": {
    "babel-plugin-import": "1.13.0",
    "customize-cra": "0.9.1",
    "husky": "4.0.3",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "react-app-rewired": "2.1.5"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,mjs,jsx,tsx,json,css,scss,md}": [
      "prettier --single-quote --trailingComma all --write",
      "git add"
    ]
  }
}
